[[inputs.prometheus]]
    urls = ["{{ url }}"]
    interval = "{{ interval }}s"
    timeout = "20s"
    response_timeout = "20s"
    [inputs.prometheus.tags]
        instance_id = "{{ instance_id }}"
        instance_type = "{{ instance_type }}"
        collect_type = "jmx"
        config_type = "jvm"
